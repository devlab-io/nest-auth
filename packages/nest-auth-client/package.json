{
  "name": "@devlab-io/nest-auth-client",
  "version": "0.0.19",
  "description": "Client library for NestJS Authentication Module",
  "author": "DevLab",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "clean": "rm -rf dist tsconfig.tsbuildinfo || true"
  },
  "dependencies": {
    "@devlab-io/nest-auth-types": "workspace:*",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.27",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
